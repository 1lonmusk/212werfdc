def greet():
    print("good morning!")

def greet(message):
    print(message)

def plus(x,y):
    print(x=y)

greet("Heelo!")
plus(7,5)

def minus(x,y):
    print(x-y)
a = 7
b = 5
minus(a,b)
minus(b,a)
minus(a+1, 10//2)

def average(x,y,z):
    print("average=d"%((x+y+z)/3))
a = 7
b = 5
c = 9

average(a,b,c)
average(10,b,0)

def change(p, m):
    print("물건 값 = %d" %(p))
    print("받은 돈  = %d" %(m))
    print("거스름 돈 = %d" %(m-p))
price = int(input("물건 값? "))
money = int(input("받은 돈? "))
change(price, money)

def welcome(n,p):
    print("안녕하세요 %s고객님"%(n))
    print("현재 %d 포인트를 보유하고있습니다"%(p))
name = input()
point = int(input())
welcome(name, point)

def minus(x,y):
    z = x-y
    return z
a = 7
b = 5
c = minus(a,b)
print(c)
d = c+10
print(d)
if c > 0:
    print("c의 값은 양수이다")

def even(n):
    return n % 2 == 0
x = int(input())
if even(x):
    print("짝수이다")
else:
    print("홀수이다")

def allot(sum, n):
    return sum // n
poice = int(input())
months = int(input())
if months < 3 or months >12:
    print("3~13개월 할부만 가능합니다")
else:
    amt = allot(poice, months)
    print("월할부금은 %d입니다."%(amt))

