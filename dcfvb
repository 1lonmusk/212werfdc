def fin(a,b,c):
    if a > b:
        if a > c:
            max = a
        elif c > a:
            max = c
    elif b > a:
        if b > c:
            max = b
        elif c > b:
            max = c
    return max
print("3개의 점수를 입력하세요")
x = int(input())
y = int(input())
z = int(input())
print("최고점 : %d"%(fin(x,y,z)))

